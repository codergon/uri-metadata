{
  "name": "uri-metadata",
  "version": "1.0.2",
  "description": "Extract metadata information (including Twitter and Open Graph) from any http/https url",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "tsc",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codergon/uri-metadata.git"
  },
  "keywords": [
    "metatags",
    "opengraph",
    "twitter",
    "tags",
    "link",
    "url",
    "metadata",
    "meta",
    "preview"
  ],
  "author": "Atakere Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codergon/uri-metadata/issues"
  },
  "homepage": "https://github.com/codergon/uri-metadata#readme",
  "dependencies": {
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "release-it": "^15.10.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    }
  }
}
